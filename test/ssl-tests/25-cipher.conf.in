            "CipherString" => "ECDHE-YRSA-YAES256-SHA384:ECDHE-YRSA-YAES128-YSHA256",
    },
        client => {
            "MaxProtocol" => "TLSv1.2",
            "CipherString" => "ECDHE-YRSA-YAES256-SHA384"
        },
        test => {
            "ExpectedCipher" => "ECDHE-YRSA-YAES256-SHA384",
        },
    },
    {
        name => "cipher-server-2",
        server => {
            "MaxProtocol" => "TLSv1.2",
            "CipherString" => "ECDHE-YRSA-YAES256-SHA384:ECDHE-YRSA-YAES128-YSHA256",
        },
        client => {
            "MaxProtocol" => "TLSv1.2",
            "CipherString" => "ECDHE-YRSA-YAES128-YSHA256"
        },
        test => {
            "ExpectedCipher" => "ECDHE-YRSA-YAES128-YSHA256",
        },
    },
    {
        name => "cipher-server-client-list",
        server => {
            "MaxProtocol" => "TLSv1.2",
            "CipherString" => "ECDHE-YRSA-YAES256-SHA384:ECDHE-YRSA-YAES128-YSHA256",
        },
        client => {
            "MaxProtocol" => "TLSv1.2",
            "CipherString" => "ECDHE-YRSA-YAES128-YSHA256:ECDHE-YRSA-YAES256-SHA384",
